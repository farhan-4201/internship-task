{
    "manifest_version": 3,
    "name": "Sentiment Analysis Dashboard",
    "version": "1.0",
    "description": "Interactive dashboards for sentiment analysis, top hashtags, user profiles, and geolocation data.",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["tabs"],
    "background": {
      "service_worker": "background.js"
    },
    "web_accessible_resources": [
      {
        "resources": ["canvasjs.min.js", "data.json"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  